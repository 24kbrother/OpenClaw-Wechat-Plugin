{
  "name": "openclaw-wechat-plugin",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "description": "企业微信（WeCom）channel plugin for OpenClaw. Supports message receiving, AI reply, media handling, and API proxy.",
  "license": "MIT",
  "exports": "./src/index.js",
  "openclaw": {
    "extensions": ["./src/index.js"],
    "channel": {
      "id": "wecom",
      "label": "WeCom",
      "selectionLabel": "WeCom (企业微信自建应用)",

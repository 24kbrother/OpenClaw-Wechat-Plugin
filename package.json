      "docsLabel": "wecom",
      "blurb": "Enterprise WeChat internal app via callback + send API.",
      "order": 80,
      "aliases": ["wework", "qiwei", "wxwork"]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/OpenClaw-Wechat-Plugin.git"
  },
  "keywords": [
    "openclaw",
    "wecom",
    "wechat",
    "enterprise-wechat",
    "chatbot",
    "ai"
  ],
  "author": "Your Name <your.email@example.com>",
  "bugs": {
    "url": "https://github.com/yourusername/OpenClaw-Wechat-Plugin/issues"
  },
  "homepage": "https://github.com/yourusername/OpenClaw-Wechat-Plugin#readme",
  "dependencies": {
    "fast-xml-parser": "^4.3.6"
  }
}
